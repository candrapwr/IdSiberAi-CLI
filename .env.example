DEEPSEEK_API_KEY=api
OPENAI_API_KEY=api
ZHIPUAI_API_KEY=api
QWEN_API_KEY=api


# =================================
# SYSTEM CONFIGURATION
# =================================

# Working Directory Configuration  
WORKING_DIRECTORY=./workspace
MAX_ITERATIONS=50

# Logging Configuration
ENABLE_LOGGING=true
LOG_RETENTION_DAYS=7

# Streaming Configuration
ENABLE_STREAMING=true

# =================================
# OPTIONAL CONFIGURATIONS
# =================================

# Default AI Provider (if multiple configured)
# Options: DeepSeek, OpenAI, Claude, Grok, ZhiPuAI, QwenAI
DEFAULT_AI_PROVIDER=DeepSeek

# Enable automatic fallback to other providers if primary fails
ENABLE_AI_FALLBACK=false

# Debug Mode
DEBUG=false
# NODE_ENV=development

# =================================
# AWS S3 CONFIGURATION
# =================================
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key
S3_REGION=your-region
S3_BUCKET=your-bucket-name
S3_ENDPOINT=endpoint-url

# Database Configuration
DB_TYPE=mysql # or 'postgres'
DB_HOST=localhost
DB_PORT=3306 # 5432 for postgres
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_database_name
